#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Users/brendanxu/Desktop/projects/greentokey/greentokey-website/node_modules/.pnpm/newman@6.2.1/node_modules/newman/bin/node_modules:/Users/brendanxu/Desktop/projects/greentokey/greentokey-website/node_modules/.pnpm/newman@6.2.1/node_modules/newman/node_modules:/Users/brendanxu/Desktop/projects/greentokey/greentokey-website/node_modules/.pnpm/newman@6.2.1/node_modules:/Users/brendanxu/Desktop/projects/greentokey/greentokey-website/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/Users/brendanxu/Desktop/projects/greentokey/greentokey-website/node_modules/.pnpm/newman@6.2.1/node_modules/newman/bin/node_modules:/Users/brendanxu/Desktop/projects/greentokey/greentokey-website/node_modules/.pnpm/newman@6.2.1/node_modules/newman/node_modules:/Users/brendanxu/Desktop/projects/greentokey/greentokey-website/node_modules/.pnpm/newman@6.2.1/node_modules:/Users/brendanxu/Desktop/projects/greentokey/greentokey-website/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../newman/bin/newman.js" "$@"
else
  exec node  "$basedir/../newman/bin/newman.js" "$@"
fi
